<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="270" android:endColor="#FAFAFA" android:startColor="#FAFAFA" />
            <corners android:radius="50dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:endColor="#83C4D1F4" android:startColor="#69C9D6F7" />
            <corners android:radius="50dp" />
        </shape>
    </item>
</selector>
